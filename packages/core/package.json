{
  "name": "@storex/core",
  "version": "1.0.5",
  "description": "Binding store to your app",
  "main": "lib/index.js",
  "scope": "@storex",
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "author": "Moshe Kabala",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "jest": "^23.3.0",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "keywords": [
      "store",
      "state",
      "common state",
      "redux",
      "flux",
      "react",
      "stencil",
      "alt",
      "observer",
      "observable",
      "mobx-state-tree"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/moshe-kabala/StoreX"
    },
    "testRegex": "\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}